{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [      
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5342"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "Application": "Megarender.WebAPI"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server={0};Port={1};Database=megarender;User Id={2};Password={3};Application Name=Megarender.WebAPI;Command Timeout=60;"
  },
  "ApplicationOptions": {
    "IdentityServiceURI": "http://{0}:{1}/"
  },
  "RabbitMQOptions": {
    "Enabled": false,
    "ConntectionString": "amqp://{0}:{1}@{2}:{3}"
  },
  "AzureStorageOptions" : {
    "Enabled": false,
    "ConnectionString" : "{0}"
  },
  "RedisSettings": {
    "Enabled": false,
    "ConnectionString": "{0}:{1}"
  }
}
